<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Add Task</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
</head>
<body class="container" style="margin-top: 50px;">
<% if (messages.error) { %>
    <div class="alert alert-danger" role="alert"><%- messages.error %></div>
<% } %>
<div class="card">
    <form action="/works/projectRegister" method="post">

        <table border="0" style="height: 168px; width: 100%; border-collapse: collapse; border-style: none;">
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4><span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">projectName</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <span style="font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">&nbsp;</span>
                    <input type="text" class="form-control" name="projectName" value="<%= projectName %>"
                           autocomplete="off">
                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4><span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">revenue</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4><span style="font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">&nbsp;</span>
                        <input type="number" class="form-control" name="revenue" value="<%= revenue %>"
                               autocomplete="off">
                    </h4>
                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4>
                        <span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">time</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4>&nbsp;
                        <input type="number" class="form-control" name="time" value="<%= time %>" autocomplete="off">
                    </h4>

                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4>
                        <span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">begin</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4>&nbsp;
                        <input type="datetime-local" class="form-control" name="begin" value="<%= begin %>"
                               autocomplete="off"></h4>

                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4><label for="end"><b>end</b></label></h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4>&nbsp;
                        <input type="datetime-local" class="form-control" name="end" value="<%= end %>"
                               autocomplete="off">
                    </h4>

                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4><span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">customer</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4>&nbsp;
                        <input type="text" class="form-control" name="customer" value="<%= customer %>"
                               autocomplete="off">
                    </h4>

                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4>
                        <span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">pm</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4>&nbsp;
                        <input type="text" class="form-control" name="pm" value="<%= pm %>" autocomplete="off"></h4>

                </td>
            </tr>
            <tr style="height: 21px;">
                <td style="width: 21.6901%; height: 21px;">
                    <h4><span style="font-weight: bold; font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;">pmEmployeeID</span>
                    </h4>
                </td>
                <td style="width: 78.3099%; height: 21px;">
                    <h4>&nbsp;
                        <input type="number" class="form-control" name="pmEmployeeID" value="<%= pmEmployeeID %>"
                               autocomplete="off"></h4>

                </td>
            </tr>
        </table>

        <p></p>
        <div class="form-group">
            <input type="submit" class="btn btn-info" value="Add"/>
        </div>
    </form>

</body>
</html>